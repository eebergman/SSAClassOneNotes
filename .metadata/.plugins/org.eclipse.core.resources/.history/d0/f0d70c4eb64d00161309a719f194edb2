package com.theironyard;

public class Person {
	String firstName;
	String middleInital;
	String lastName;

	public Person() {
		this.firstName = "Rodney";
		this.middleInital = "P";
		this.lastName = "Barbarti";
	}

	public String getFullName() {
		return firstName + " " + middleInital + " " + lastName;
	}

}
